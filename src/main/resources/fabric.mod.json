{
	"schemaVersion": 1,
	"id": "era-of-rails",
	"version": "${version}",
	"name": "Era of Rails",
	"description": "A railway expansion mod!",
	"authors": [
		"Farpo",
		"DinoTheOogle"
	],
	"accessWidener" : "era-of-rails.accesswidener",
	"contact": {
		"homepage": "https://nexusrealms.de/",
		"sources": "https://github.com/Nexusrealms/era-of-rails"
	},
	"license": "MIT",
	"icon": "assets/era-of-rails/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"de.nexusrealms.eraofrails.EraOfRails"
		],
		"client": [
			"de.nexusrealms.eraofrails.client.EraOfRailsClient"
		],
		"fabric-datagen": [
			"de.nexusrealms.eraofrails.datagen.EraOfRailsDataGenerator"
		]
	},
	"mixins": [
		"era-of-rails.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"loom:injected_interfaces": {
			"net/minecraft/class_1688": ["de/nexusrealms/eraofrails/entity/CartLinker"],
			"net/minecraft/class_10045": ["de/nexusrealms/eraofrails/client/CartLinkerRenderState"]
		}
	}
